DELIMITER $$
DROP PROCEDURE IF EXISTS loopInsert$$
 
CREATE PROCEDURE loopInsert()
BEGIN
    DECLARE i INT DEFAULT 1;
        
    WHILE i <= 5 DO
        insert into comment(parent_num,content, created, post_id)
VALUES(2,"post 3번에써져있는 2번 댓글의 자식글입니다", now() , 3);
        SET i = i + 1;
    END WHILE;
END$$
DELIMITER $$


CALL loopInsert;


-- user
-- user_id, password, email, nickname, img_url, img_name, description, activated
-- int,     varchar, varchar, varchar, text,    text,       text,       tinyint

insert into user(password, email, nickname, description, activated)
VALUES("user3", "user3@naver.com", "user3계정", "user3계정 소개 입니다", 1)

-- post
-- post_id, title, content, img_url, img_name, created, modified, like, user_id
-- int,     varchar, text,  text,       text,  datetiem, datetime, int,     int

insert into post(title, content, created, `like`, user_id)
VALUES("user2가 작성한 post test", "user 계정으로 작성한 test post 입니다.", "2018-04-10", 24, 2);

-- comment
-- comment_id, parent_num, content, created, modified, like, user_id, post_id
--  int,            int,    text,   datetime, datetime, int,    int,    int

insert into comment(parent_num,content, created, post_id)
VALUES(2,"post 3번에써져있는 2번 댓글의 자식글입니다", now() , 3);